{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ModelSettings": {
    "TokenizerConfigPath": "Vocabularys/tokenizer.json", // Caminho relativo à raiz do projeto
    "MaxSequenceLength": 50,
    "EmbeddingSize": 128,
    "VocabSizeLimit": 60000, //Limite superior para o tokenizer
    "MaxTreeDepth": 5,
    "TrainingEpochs": 20 // Mover epochs para cá também é uma boa ideia
  }
}
